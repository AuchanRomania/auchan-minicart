{
  "minicart": {
    "required": ["product-summary"],
    "allowed": ["sandbox"],
    "component": "MinicartLegacy"
  },
  "minicart-layout": {
    "component": "Minicart",
    "composition": "children",
    "allowed": "*",
    "render": "client"
  },
  "minicart-base-content": {
    "component": "BaseContent",
    "allowed": ["minicart-product-list", "minicart-summary"]
  },
  "minicart-product-list": {
    "component": "ProductList",
    "allowed": ["product-list"]
  },
  "minicart-summary": {
    "component": "Summary",
    "allowed": ["checkout-summary"]
  }
}
